<%= form_for @suggestion, url: suggestions_path do |f| %>
	<%= render 'shared/error_messages', :object=>f.object %>
	
	<div>
		<%= f.label :title %>
		<%= f.text_field :title %>
	</div>
	
	<div>
			<%= f.label :content %>
			<%= f.text_field :content %>
	</div>
		
	<div>
		<%= f.submit 'Save' %>
	</div>
	
<% end %> 