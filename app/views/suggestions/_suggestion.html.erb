<div>
	<%= suggestion.title %>: 
	<%= link_to 'Show', suggestion_path(id:suggestion.id) %> 
	<% if current_user.is_member %> 
	| <%= link_to 'Edit', edit_suggestion_path(id:suggestion.id) %>
	| <%= link_to 'Delete', suggestion, :method=>:delete, :data=>{:confirm=>'Are you sure?'} %>
	<% end %>
</div>